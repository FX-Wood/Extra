
<h2> Add new card </h2>
<select name="collection-picker" id="collection-picker">
<%user.collections.forEach(collection => { %>
    <option value="<%= collection.id %>"> <%= '[' + collection.id + '] ' + collection.name%></option>
<% }) %>
</select>
<div class="container">

<div class="row">
    
    <div class="col s12 m6">
        <div class="card">
            <div class="card-content">
                <span class="card-title"> Front of card </span>
            </div>
        </div>
    </div>
    
    <div class="col s12 m6">
        <div class="card">
            <div class="card-content">
                <span> Back of card</span>
            </div>
        </div>
    </div>

</div>
</div>