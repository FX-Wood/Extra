<% if (alerts.error) { %>
    <% alerts.error.forEach(msg => { %>
        <script>
                console.log('inside the alerts.error block')
                console.log(M)
                M.toast({'html': '<%= msg %>'})
        </script>
    <% }) %>
<% } %>

<% if (alerts.success) { %>
    <% alerts.success.forEach(msg => { %>
        <script>
            console.log('inside the alerts.success block')
            console.log(M)
            M.toast({'html': '<%= msg %>'})
        </script>
    <% }) %>
<% } %>